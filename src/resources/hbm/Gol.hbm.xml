<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="hibereuro2012.Gol" table="goal">
      <composite-id>
        <key-property name="juego" column="matchid" />
        <key-property name="minuto" column="gtime" />
      </composite-id>
      <property name="equipo" column="teamid" />
      <property name="jugador" column="player" />
      <property name="minuto" column="gtime" />
  </class>
</hibernate-mapping>

